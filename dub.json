{
	"name": "gen-package-version",
	"description": "Generate a D module with version information automatically-detected from git.",
	"authors": ["Nick Sabalausky"],
	"homepage": "https://github.com/Abscissa/gen-package-version",
	"license": "zlib/libpng",
	"targetType": "executable",
	"targetPath": "bin",
	"targetName": "gen-package-version",
	"sourcePaths": ["src"],
	"dependencies": {
		"scriptlike": "~>0.8.0"
	},
	"preGenerateCommands-posix":
		["cd $PACKAGE_DIR && chmod +x helper/bootstrap.sh && helper/bootstrap.sh $SCRIPTLIKE_PACKAGE_DIR"],
	"preGenerateCommands-windows":
		["cd $PACKAGE_DIR && helper\\bootstrap.cmd $SCRIPTLIKE_PACKAGE_DIR"]
}
